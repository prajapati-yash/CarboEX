{
  "expo": {
    "name": "CarboEx",
    "slug": "CarboEx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/carboex.png",
    "userInterfaceStyle": "light",
    "scheme": "walletconnect-example",
    "plugins": ["./app.plugin"],
    "splash": {
      "image": "./assets/carboex.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carboex"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/carboex.png",
        "backgroundColor": "#000"
      },
      "package": "com.carboex"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "77ce727b-d5aa-404f-817f-8eaa6ea154d1"
      }
    }
  }
}
